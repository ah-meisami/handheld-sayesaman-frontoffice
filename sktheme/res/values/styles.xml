    <style name="txt_Suppliyer_type">
        <item name="android:textColor">#ff000000</item>
    </style>
     <style name="imagebuttonStyle">
        <item name="android:background">@drawable/imagebuttonbg</item>
    </style>
    <style name="txt_account_view">
        <item name="android:background">@drawable/txt_bg</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:gravity">right</item>
    </style>
    <style name="badger">
        <item name="android:minHeight">10dp</item>
        <item name="android:minWidth">10dp</item>
        <item name="android:background">@drawable/badger</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:clickable">false</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>